o el grafico\n","fig = go.Figure()\n","\n","# Define una función para actualizar el gráfico según la fecha seleccionada\n","def update_bar_chart(selected_date):\n","    filtered_df = df[df['fecha'] == selected_date]\n","    fig.data = []  # Limpia los datos anteriores\n","    fig.add_trace(go.Bar(x=filtered_df['aplicación'], y=filtered_df['aperturas'], name='aperturas', marker_color='red'))\n","    fig.add_trace(go.Bar(x=filtered_df['aplicación'], y=filtered_df['minutos'], name='minutos', marker_color='blue'))\n","\n","# Agregar los datos iniciales\n","update_bar_chart(df['fecha'].min())\n","\n","# Personalizar el diseño\n","fig.update_xaxes(title=\"aplicación\")\n","fig.update_yaxes(title=\"Cantidad / Tiempo\")\n","fig.update_layout(legend_title_text=\"Datos\")\n","\n","# Crear un slider de fechas\n","date_slider = go.FigureWidget(\n","    data=[go.Scatter(x=df['fecha'], y=[1] * len(df), mode='markers', marker_opacity=0)],\n","    layout=go.Layout(\n","        xaxis=dict(range=[df['fecha'].min(), df['fecha'].max()]),\n","        xaxis_title=\"fecha\",\n","        sliders=[{\n","            \"steps\": [{\"args\": [date], \"label\": date, \"method\": \"relayout\"} for date in df['fecha']],\n","            \"active\": 0,\n","            \"currentvalue\": {\"font\": {\"size\": 12}, \"prefix\": \"fecha: \", \"visible\": True}\n","        }],\n","    )\n",")\n","\n","fig_widget = go.FigureWidget(fig)\n","#fig_widget.layout['updatemodes'] = ['draw', 'lasso', 'select', 'zoom', 'pan', 'reset', 'autoscale']\n","#fig_widget.update_layout(updatemodes=[\"draw\", \"lasso\", \"select\", \"zoom\", \"pan\", \"reset\", \"autoscale\"])\n","fig_widget.update_layout(\n","    updatemodes=[\"draw\", \"lasso\", \"select\", \"zoom\", \"pan\", \"reset\", \"autoscale\"]\n",")\n","\n","fig_widget.layout.yaxis.fixedrange = True\n","fig_widget.layout.xaxis.fixedrange = True\n","\n","date_slider_widget = go.FigureWidget(date_slider)\n","date_slider_widget.layout.yaxis.fixedrange = True\n","\n","from plotly.subplots import make_subplots\n","\n","# Crear el diseño final con el gráfico y el slider\n","subplots = make_subplots(rows=2, cols=1, shared_xaxes=True, vertical_spacing=0.1)\n","subplots.add_trace(fig_widget.data[0], row=1, col=1)\n","subplots.add_trace(date_slider_widget.data[0], row=2, col=1)\n","subplots.update(fig_widget.layout)\n","\n","# Mostrar el gráfico\n","subplots.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"h3DGSeZ244mp","executionInfo":{"status":"error","timestamp":1698979321519,"user_tz":180,"elapsed":676,"user":{"displayName":"jorge galvez","userId":"06372880474160366985"}},"outputId":"d81651c5-25d9-4268-a9aa-9df3a06f1525"},"execution_count":77,"outputs":[{"output_type":"error","ename":"ValueError","evalue":"ignored","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mValueError\u001b[0m                                Traceback (most recent call last)","\u001b[0;32m<ipython-input-77-9eac359336fc>\u001b[0m in \u001b[0;36m<cell line: 42>\u001b[0;34m()\u001b[0m\n\u001b[1;32m     40\u001b[0m \u001b[0;31m#fig_widget.layout['updatemodes'] = ['draw', 'lasso', 'select', 'zoom', 'pan', 'reset', 'autoscale']\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m     41\u001b[0m \u001b[0;31m#fig_widget.update_layout(updatemodes=[\"draw\", \"lasso\", \"select\", \"zoom\", \"pan\", \"reset\", \"autoscale\"])\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0;32m---> 42\u001b[0;31m fig_widget.update_layout(\n\u001b[0m\u001b[1;32m     43\u001b[0m     \u001b[0mupdatemodes\u001b[0m\u001b[0;34m=\u001b[0m\u001b[0;34m[\u001b[0m\u001b[0;34m\"draw\"\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0;34m\"lasso\"\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0;34m\"select\"\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0;34m\"zoom\"\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0;34m\"pan\"\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0;34m\"reset\"\